	journal = {Proceedings of the National Academy of Sciences},
	month = jul,
	note = {Publisher: Proceedings of the National Academy of Sciences},
	number = {31},
	pages = {8043--8048},
	title = {Amygdala lesions eliminate viewing preferences for faces in rhesus monkeys},
	url = {https://www.pnas.org/doi/full/10.1073/pnas.1807245115},
	urldate = {2023-05-08},
