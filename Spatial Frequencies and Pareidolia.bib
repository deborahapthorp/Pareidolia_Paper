	month = feb,
	note = {Publisher: SAGE Publications Inc},
	number = {2},
	pages = {566--574},
	title = {Rapid {Fear} {Detection} {Relies} on {High} {Spatial} {Frequencies}},
	url = {https://doi.org/10.1177/0956797613512509},
